{
  "name": "@openmrs/esm-extension-manager",
  "version": "0.2.0",
  "description": "Coordinates extensions and extension points in the OpenMRS Frontend",
  "browser": "dist/openmrs-esm-extension-manager.js",
  "main": "src/index.ts",
  "source": true,
  "scripts": {
    "test": "jest --passWithNoTests",
    "start": "webpack-dev-server",
    "build": "rimraf dist && concurrently -n webpack,typescript 'webpack --mode=production' 'npm run typescript'",
    "typescript": "tsc",
    "prettier": "prettier 'src/**/*' --write",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext ts"
  },
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-core.git"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-core/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-core#readme",
  "keywords": [
    "openmrs"
  ],
  "license": "MPL-2.0",
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x",
    "single-spa": "4.x"
  },
  "devDependencies": {
    "@types/react": "^16.9.46",
    "react": "^16.13.1",
    "single-spa": "^4.4.1",
    "systemjs-webpack-interop": "^2.1.2"
  }
}
