{
  "name": "@openmrs/esm-config",
  "version": "2.2.0",
  "license": "MPL-2.0",
  "description": "A configuration library for the OpenMRS Single-Spa framework.",
  "browser": "dist/openmrs-esm-module-config.js",
  "main": "src/index.ts",
  "source": true,
  "scripts": {
    "clean": "rimraf dist",
    "document": "./document.sh",
    "start": "webpack-dev-server",
    "start-https": "webpack-dev-server --https",
    "test": "BABEL_ENV=test jest",
    "lint": "eslint src --ext ts,tsx",
    "typescript": "tsc",
    "build": "rimraf dist && concurrently -n webpack,typescript 'webpack --mode=production' 'npm run typescript'",
    "prepublishOnly": "npm run build"
  },
  "directories": {
    "lib": "dist"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-core.git"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-core/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-core#readme",
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "peerDependencies": {
    "react": "16.x",
    "single-spa": "4.x"
  },
  "devDependencies": {
    "@types/react": "^16.9.46",
    "@types/ramda": "^0.26.44",
    "@types/systemjs": "^6.1.0",
    "react": "^16.13.1",
    "babel-plugin-ramda": "^2.0.0",
    "single-spa": "^4.4.1",
    "systemjs-webpack-interop": "^2.1.2"
  }
}
